package jp.co.ll;

public class SmartPhone extends Mp3Player{
   public void call(){
	   System.out.println("電話 (ハイレゾ機能) ");
   }

public void mail(){
	System.out.println("メール (ハイレゾ機能) ");
}
public void photo(){
	System.out.println("写真 (ハイレゾ機能) ");
}

public void internet(){
	System.out.println("インターネット (ハイレゾ機能) ");
}
}